    "import time\n",
    "def countdown(t):\n",
    "    while t:\n",
    "        minutes,seconds=divmod(t,60)\n",
    "        print(minutes,seconds, end=\"\\r\")\n",
    "        time.sleep(1)\n",
    "        t=t-1\n",
    "t= int(input (\"   enter time in seconds   \"))\n",
    "(countdown(t))\n",
    "print(\"fire in the hole\")\n"
   ]
